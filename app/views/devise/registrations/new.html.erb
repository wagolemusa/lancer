<div class="contents">
  <section class="access">
  <br>
    <div class="container">
      <div class="content-inner-detail">
<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: "login-form clearfix"}) do |f| %>
  <%= devise_error_messages! %>

  <div class="form-group">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, class: "form-control input-lg" %>
  </div>
  <div class="form-group">
    <%= f.label :name %><br />
    <%= f.text_field :name, class: "form-control input-lg" %>
  </div>


  <div class="form-group">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off", class: "form-control input-lg" %>
  </div>

  <div class="form-group">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control input-lg" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up", class: "btn btn-primary input-lg btn-block" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
</div>
</div>
</section>
</div>